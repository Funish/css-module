{
  "name": "@funish/css",
  "version": "0.0.1-alpha.10",
  "description": "A concise front-end CSS framework.",
  "main": "dist/funish.min.css",
  "files": [
    "*README*",
    "/src",
    "/dist"
  ],
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "format": "prettier --write \"./**/*.{vue,js,json,md}\"",
    "complete": "sass src/funish.scss dist/funish.css --no-source-map",
    "compressed": "sass src/funish.scss dist/funish.min.css --style=compressed --no-source-map",
    "highlight": "sass src/highlight.js.scss dist/funish-hl.css --style=compressed --no-source-map",
    "output": "yarn complete && yarn compressed && yarn highlight",
    "test": "yarn generate && yarn output",
    "prepublish": "yarn output",
    "prerelease": "yarn output & yarn version --new-version prerelease & yarn publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Funish/css-module.git"
  },
  "keywords": [
    "concise",
    "modular",
    "component",
    "front-end",
    "css",
    "framework"
  ],
  "author": "Funish",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Funish/css-module/issues"
  },
  "homepage": "https://css.funish.net",
  "dependencies": {
    "sass": "^1.26.10",
    "sass-loader": "^8.0.2"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@nuxtjs/markdownit": "^1.2.9",
    "nuxt": "^2.0.0",
    "prettier": "2.0.5"
  }
}
